<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head') %>
    <title>Login</title>
</head>


<body id="login">
  <figure>
    <img
      src="https://images.unsplash.com/photo-1488707872600-5507977fe355?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1770&q=80"
      load= "lazy" alt="">
  </figure>

  <section>
    <form action="/login" method="post">
      <legend>Login</legend>
      <div>
        <label for="username">Username</label>
        <input type="username" id="username" name="username" placeholder="Alcatrazss" />
      </div>
      <div>
        <label for="password">Password</label>
        <input type="password" id="password" name="password" required placeholder="Wachtwoord"/>
      </div>
      <button type="submit">Login</button>
    </form>
  </section>


  <!-- Link javascript -->

</body>

</html>